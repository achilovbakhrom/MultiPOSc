<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/total_starting_amount_"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvStartingCash"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/pay_outs"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvPayOuts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/pay_ins"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvPayIns"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/pay_to_vendor"
            android:textColor="@color/colorMainText" />


        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvPayToVendors"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/income_debt"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvIncomeDebt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/twenty_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/bank_drops"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvBankDrops"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/real_product_sales"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvCashTransactions"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/tips_dispensed"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvTips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ten_dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/fifty_dp"
            android:text="@string/expected_amount_in_till"
            android:textColor="@color/colorMainText" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="5px"
            android:layout_gravity="bottom"
            android:layout_marginBottom="@dimen/three_dp"
            android:layout_weight="1"
            android:layerType="software"
            android:src="@drawable/dash" />

        <TextView
            android:id="@+id/tvExpectedCash"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/five_dp"
            android:text="0"
            android:textColor="@color/colorMainText" />
    </LinearLayout>

</LinearLayout>